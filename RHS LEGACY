Rainbow.TextSize = 16.000
Rainbow.TextStrokeTransparency = 0.000
Rainbow.MouseButton1Down:connect(function()
while wait() do
		for i,v in pairs(game.Teams:GetChildren()) do
	    	if v.Name == "Principal" then wait() else wait()
				game:GetService("ReplicatedStorage").RemoteFunctions.ChangeTeam:FireServer(v.Name);
			end
		end
		end
end)

