local function VLZR_fake_script() -- mainframe.LocalScript 
	local script = Instance.new('LocalScript', mainframe)

	game:GetService("ReplicatedStorage").RemoteFunctions.LocalNotification:Fire("Gui made by chillin aka mac#7174");
	wait(3)
	game:GetService("ReplicatedStorage").RemoteFunctions.LocalNotification:Fire("Press E To Open Gui");
	wait(3)
	game:GetService("ReplicatedStorage").RemoteFunctions.LocalNotification:Fire("Remove RP name Before Executing RainbowName");
	--Le Chillin Outfit
	local WearOutfit = game:GetService("ReplicatedStorage").RemoteFunctions.WearOutfit
	WearOutfit:InvokeServer(
		{
			["Hat3"] = "",
			["Hat1"] = "191101707",
			["RemoveShirt"] = false,
			["Face"] = "180660043",
			["Hat2"] = "",
			["Shirt"] = "",
			["Pants"] = "",
			["RPDesc"] = "",
			["RPName"] = "RIP",
			["OutfitName"] = "CHILLINWUZHERE"
		}
	)
end
coroutine.wrap(VLZR_fake_script)()
