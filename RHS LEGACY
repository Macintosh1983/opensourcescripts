_G.WS = SPEED.Text;
				local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid;--credit to @Matt_Lawz#2170 
				Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
				Humanoid.WalkSpeed = _G.WS;
				end)
				Humanoid.WalkSpeed = _G.WS;
end)

